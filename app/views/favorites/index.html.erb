<div class="container">
    <div class="row">
        <p>いいね一覧</p>
    </div>
    <div class="row">
        <% @current_user.favorites.each do |favorite| %>
        <div class="col-md-3">
            <%= link_to post_image_path(favorite.post_image.id) do %>
            <%= attachment_image_tag favorite.post_image, :image, :size =>'200x200' %>
            <% end %>
        </div>
        <% end %>
    </div>
</div>